<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="default" namespace="/" extends="struts-default">
		<global-results>
			<result name="login">/WEB-INF/SystemUser/login.jsp</result>
		</global-results>
	
	 
		<action name="userAction_*" class="cn.ssha.action.UserAction" method="{1}">
			<result name="index">/WEB-INF/index.jsp</result>
			<result name="top">/WEB-INF/top.jsp</result>
			<result name="bottom">/WEB-INF/bottom.jsp</result>
			<result name="left">/WEB-INF/left.jsp</result>
			<result name="right">/WEB-INF/right.jsp</result>
		</action>
	
	</package>	
</struts>
